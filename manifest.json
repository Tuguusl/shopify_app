{
  "author": {
    "name": "Zendesk",
    "email": "support@zendesk.com"
  },

  "location": ["ticket_sidebar", "new_ticket_sidebar"],
  "defaultLocale": "en",
  "private": false,
  "singleInstall": false,
  "frameworkVersion": "1.0",
  "version": "1.0.1",

  "parameters": [
    {
      "name": "url",
      "type": "url",
      "required": true
    },
    {
      "name": "access_token",
      "type": "hidden"
    },
    {
      "name": "items_purchased",
      "type": "checkbox"
    },
    {
      "name": "item_quantity",
      "type": "checkbox"
    },
    {
      "name": "items_price",
      "type": "checkbox"
    },
    {
      "name": "storefront_name",
      "type": "checkbox"
    },
    {
      "name": "purchase_total",
      "type": "checkbox"
    },
    {
      "name": "order_status",
      "type": "checkbox"
    },
    {
      "name": "date_ordered",
      "type": "checkbox"
    },
    {
      "name": "shipping_address",
      "type": "checkbox"
    },
    {
      "name": "shipping_method",
      "type": "checkbox"
    },
    {
      "name": "billing_address",
      "type": "checkbox"
    },
    {
      "name": "customer_notes",
      "type": "checkbox"
    },
    {
      "name": "order_notes",
      "type": "checkbox"
    },
    {
      "name": "order_id_field_id",
      "type": "number",
      "required": false
    }
  ]
}
