{
  "author": {
    "name": "Zendesk",
    "email": "support@zendesk.com"
  },

  "location": {
    "zendesk": {
      "ticket_sidebar": "assets/iframe.html",
      "new_ticket_sidebar": "assets/iframe.html"
    }
  },
  "defaultLocale": "en",
  "private": false,
  "singleInstall": false,
  "frameworkVersion": "2.0",
  "version": "2.0.0",

  "parameters": [
    {
      "name": "url",
      "type": "url",
      "required": true
    },
    {
      "name": "items_purchased",
      "type": "checkbox",
      "default": true,
      "required": true
    },
    {
      "name": "item_quantity",
      "type": "checkbox",
      "default": true,
      "required": true
    },
    {
      "name": "item_price",
      "type": "checkbox",
      "default": false,
      "required": true
    },
    {
      "name": "storefront_name",
      "type": "checkbox",
      "default": false,
      "required": true
    },
    {
      "name": "purchase_total",
      "type": "checkbox",
      "default": true,
      "required": true
    },
    {
      "name": "order_status",
      "type": "checkbox",
      "default": true,
      "required": true
    },
    {
      "name": "date_ordered",
      "type": "checkbox",
      "default": true,
      "required": true
    },
    {
      "name": "shipping_address",
      "type": "checkbox",
      "default": true,
      "required": true
    },
    {
      "name": "shipping_method",
      "type": "checkbox",
      "default": false,
      "required": true
    },
    {
      "name": "billing_address",
      "type": "checkbox",
      "default": false,
      "required": true
    },
    {
      "name": "customer_notes",
      "type": "checkbox",
      "default": false,
      "required": true
    },
    {
      "name": "order_notes",
      "type": "checkbox",
      "default": false,
      "required": true
    },
    {
      "name": "access_token",
      "type": "hidden",
      "required": false
    }
  ]
}
